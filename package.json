{
  "scripts": {
    "install_client": "cd ./github-search-client && npm install",
    "install_server": "cd ./GitHubSearch.API && dotnet restore",
    "install_all": "run-p install_client install_server",
    "start_client": "cd ./github-search-client && npm run start",
    "start_server": "cd ./GitHubSearch.API && dotnet run",
    "start_all": "run-p start_server start_client"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
